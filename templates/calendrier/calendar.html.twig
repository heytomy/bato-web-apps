{% extends '_inc/listPage.html.twig' %}

{% block new_button %}
	<a class="btn btn-primary" href="{{ path('app_calendrier_new') }}">
		<i class="fa-solid fa-chevron-right"></i>
		Nouveau Rendez-vous
	</a>
{% endblock %}

{% block list %}
    <div 
    id="calendar-holder"
    data-events-url="{{ path('fc_load_events') }}"></div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('calendrier') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('calendrier') }}
{% endblock %}