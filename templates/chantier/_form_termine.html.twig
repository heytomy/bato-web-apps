<div class="container-fluid">
	<div class="card border rounded-5 bg-light p-4 mt-4">
		{{ form_start(form) }}
		<div class="row mt-3 ">
            <div class="col">{{ form_row(form.libelle) }}</div>
		</div>

		<div class="row mt-4">
			<div class="col">{{ form_row(form.description) }}</div>
		</div>

		<button type="submit" class="btn btn-primary mt-3">{{ button_label|default('Enregistrer') }}</button>
		{{ form_end(form) }}
	</div>
</div>
